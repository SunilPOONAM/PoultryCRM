                <a href="javascript:void(0);" class="bars">
                   
                </a>
                <a class="navbar-brand" href="/Master/Index">Farm User - MATERIAL </a>
            </div>
            <div class="collapse navbar-collapse" id="navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    <!-- Call Search -->
                    <li>
